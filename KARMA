                pcall(function()
                    game.Players.LocalPlayer.Character["[Chicken]"]:Activate()
                    wait(0.1)
                    game.Players.LocalPlayer.Character["[Chicken]"]:Deactivate()
                end)
            end
        end)
    end
    end
