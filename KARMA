local new = { 
    main = { 
        Mario = true,
        Part = "HumanoidRootPart", -- Head, UpperTorso, HumanoidRootPart, LowerTorso, RightFoot, LeftFoot, RightArm, LeftArm 
        Key = "q",
        Notifications = true,
    },
